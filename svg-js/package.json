{
  "name": "svg-js",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "babel": "^6.23.0",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0"
  },
  "devDependencies": {
    "html-webpack-plugin": "^5.3.1",
    "terser-webpack-plugin": "^5.1.2",
    "webpack-dev-server": "^3.11.2"
  },
  "scripts": {
    "build": "NODE_ENV=production npx webpack",
    "size:js": "cat dist/*.js | wc -c && gzip -c dist/*.js | wc -c",
    "size:inline": "cat dist/inline.html | wc -c && gzip -c dist/inline.html | wc -c",
    "size": "npm run build && npm run size:js && npm run size:inline"
  }
}
